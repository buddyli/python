body {
	background: #BEB7A9 url('../images/coffee_bg.gif') repeat-y top left;
}



#sidebar {
	background: none;
	border-right: 1px solid #D0C9BB;
}

.logo { background: url('../images/coffee_bg_logo.gif') repeat-x top left; }
.top_nav { background: url('../images/coffee_bg_header.gif') repeat-x top left; }

.title a { color: #575247; }
.session { color: #766E5F; }
.session a { color: #989F30; }

#navigation a { color: #BEB7A9; }
#navigation a:hover { color: #D1CABB; }

.navhead, .navhead_blank {
	background: #867E6E url('../images/coffee_bg_navhead.gif') repeat-x top left;
	border: 1px solid #8A8271;
	color: #BEB7A9;
}
	.navhead:hover, .navhead_blank:hover { border: 1px solid #9B9280; }

.subnav {
	background: #69655B;
}

.navbullet {
	background: url('../images/coffee_navbullet.png') no-repeat;
}

.submenu li a { border-bottom: 1px solid #757168; }



#copyrights {
	background: #615C52 url('../images/coffee_splash.gif') no-repeat 8px;
	color: #807A6F;
}
#copyrights a { color: #807A6F; }



#content {
	background: #C5BEB2;
}
	.box {
		background: #fff url('../images/bg_box.gif') repeat-x top left;
		border: 1px solid #fff;
	}
	
tbody tr:hover td { background: #FDF9F3; }

/* Code & Pre */

code,
pre {
	border-left: 4px solid #C5BEB2;
}

/* Forms */

.submit:hover,
.button:hover {
	color: #70A235;
}

/* Pagination */
/* Class .current is used to mark current page; */

.paginator li a:hover, .paginator li.current a {
	color: #545454;
	border: 1px solid #BEB7A9;
	background: #F2EADD;
}